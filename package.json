        "@typescript-eslint/eslint-plugin": "5.1.0",
        "@typescript-eslint/parser": "5.1.0",
        "eslint": "8.1.0",
        "jest": "27.3.1",
        "prettier": "2.4.1",
        "ts-jest": "27.0.7",
        "typescript": "4.4.4"
    },
    "jest": {
        "preset": "ts-jest",
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        }
    }
